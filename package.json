  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --ignore \"./vite.config.ts.*\" apps/server/index.ts",
    "build": "rimraf dist && node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build && tsc --noEmit && esbuild apps/server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/apps/server && shx cp -r apps/server/swagger dist/apps/server/swagger",
    "start": "cross-env NODE_ENV=production node dist/apps/server/index.js",
    "db:push": "drizzle-kit push",
    "db:seedTest": "tsx apps/server/db/seedTest.ts",
    "db:seedWork": "tsx apps/server/db/seedWork.ts",
    "db:seedPallets": "tsx apps/server/db/seedPalletPositions.ts",
    "db:seedUpdate": "tsx apps/server/db/seedUpdatePalletLogs.ts",
    "check": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
